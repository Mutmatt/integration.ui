<div data-ng-cloak>
    <ul class="select-card-list">
        <li class="select-card-container" data-ng-class="at.getTestResults(testSuite)" data-ng-repeat="testSuite in at.testSuites" data-ng-click="at.selectTestSuite(testSuite)">
            <h1>{{testSuite.Name}}</h1>
            <p>Passed Count: <strong>{{at.getTestStatusCount(true, testSuite)}}</strong></p>
            <p>Failed Count: <strong>{{at.getTestStatusCount(false, testSuite)}}</strong></p>

        </li>
    </ul>
    <div data-ui-view="">
    </div>
</div>