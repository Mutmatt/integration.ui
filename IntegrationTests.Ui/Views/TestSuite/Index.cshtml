<div class="overlay">
    <i class="close fa fa-times" tabindex="-1" data-ui-sref="Test"></i>
    <div class="col-xs-12 col-lg-offset-3 col-lg-6 overlay-center">
        <ul class="select-card-list">
            <li class="select-card-container" data-ng-class="ts.getTestResults(test)" data-ng-repeat="test in ts.tests" data-ng-click="ts.selectTest(test)">
                <h1>{{test.Name}}</h1>
                <p>Passed Count: <strong>{{test.PassCount}}</strong></p>
                <p>Failed Count: <strong>{{test.FailCount}}</strong></p>
            </li>
        </ul>
    </div>
    <div data-ui-view=""></div>
</div>